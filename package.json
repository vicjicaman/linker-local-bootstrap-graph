{
  "name": "@nebulario/linker-local-boostrap-graph",
  "version": "1.70.5-master",
  "description": "GraphQL for local boostrap",
  "main": "dist/index.js",
  "repository": "github.com:vicjicaman/linker-auth-graph.git",
  "author": "Victor Jimenez <vic@repoflow.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/runtime": "^7.6.0",
    "@babel/runtime-corejs3": "^7.6.0",
    "@nebulario/linker-logger": "1.70.0-master",
    "@nebulario/linker-utils": "1.70.0-master",
    "axios": "^0.19.0",
    "child-process-promise": "^2.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.2.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tools": "^4.0.4",
    "graphql-tools-types": "^1.2.1",
    "lodash": "^4.17.15"
  },
  "scripts": {
    "clean": "rm -Rf ./dist*",
    "start:prod": "NODE_ENV=\"production\" node ./dist/index.js",
    "start:dev": "NODE_ENV=\"development\" nodemon ./dist/index.js",
    "build:prod": "yarn clean && webpack --config webpack.config.js --mode=production ",
    "build:dev": "yarn clean && webpack --config webpack.config.js --mode=development ",
    "build:watch:dev": "yarn build:dev --watch",
    "build:watch:prod": "yarn build:prod --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "babel-loader": "^8.0.6",
    "nodemon": "^1.19.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-node-externals": "^1.7.2"
  }
}
